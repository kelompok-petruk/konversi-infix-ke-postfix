#include<iostream>
#include<stdio.h>
#include<stdlib.h> // untuk keluar ()
#include<ctype.h>  //untuk isdigit (char)
#include<string.h>
#define SIZE 100 // ditanyakan disini sebagai variabel gkobal karena tumpuka [] di gunakan lebih dari satu fungsi
using namespace std;

char stack [SIZE];
int top = -1;
//tentukan operasi push
void push(char item) {
if (top >= SIZE -1){
cout<<"\nStack Overflow.";
}
else{
top =top+1;
stack[top] =item;
}
}




